<main class="page login-page">
    <section class="clean-block clean-form dark">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Log In</h2>
               
                    <p class="error">Sorry, the member name and password
 you entered do not match. Please try again.</p>
            </div>
            <form class="login">
                <div class="form-group"><label for="email">Email</label><input type="email" class="form-control item"
                        id="email" /></div>
                <div class="form-group"><label for="password">Password</label><input type="password"
                        class="form-control" id="password" /></div>
                <div class="form-group">
                    <div class="form-check"><input type="checkbox" class="form-check-input" id="checkbox" /><label
                            class="form-check-label" for="checkbox">Remember me</label></div>
                </div><button class="btn btn-primary btn-block" type="submit">Log In</button>
            </form>
        </div>
    </section>
</main>
